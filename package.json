{
  "name": "mailer-send-ts",
  "version": "1.4.0",
  "description": "Typescript package for MailerSend",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build"
  },
  "author": {
    "name": "Iaromir Trinta",
    "email": "iarki90@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/IARKI/mailer-send-ts/issues"
  },
  "homepage": "https://github.com/IARKI/mailer-send-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IARKI/mailer-send-ts.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.5",
    "nock": "^13.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "got": "^11.8.2"
  },
  "keywords": ["mailersend", "typescript", "mailer-send", "types"]
}
